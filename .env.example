# Data.gov.in API key for crime dataset
NEXT_PUBLIC_DATAGOV_KEY=your_datagov_api_key_here

# Optional: Resource ID for crime dataset
# Example: NEXT_PUBLIC_DATAGOV_RESOURCE_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
NEXT_PUBLIC_DATAGOV_RESOURCE_ID=5f177a8b-0fdd-4088-b6e6-a35babdac820

# Firebase Web App Configuration
# Get these values from Firebase Console > Project Settings > Your apps > Web app config
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
NEXT_PUBLIC_FIREBASE_APP_ID=1:your_app_id:web:your_web_config_id

# Optional: Genkit API key
GENKIT_GOOGLE_API_KEY=your_genkit_api_key
